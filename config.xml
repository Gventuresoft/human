<?xml version='1.0' encoding='utf-8'?>
<widget id="com.airtouchmedia.humaneland" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Human WS</name>
    <description>
		Human WS
	</description>
    <author email="devs@airtouchmedia.com" href="https://www.airtouchmedia.com">
		Airtouch New Media
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="disallowoverscroll" value="true" />
    <preference name="target-device" value="handset" />
    <preference name="Orientation" value="default" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="res/android/icon/48.png" />
        <icon density="hdpi" src="res/android/icon/72.png" />
        <icon density="xhdpi" src="res/android/icon/96.png" />
        <icon density="xxhdpi" src="res/android/icon/144.png" />
        <icon density="xxxhdpi" src="res/android/icon/192.png" />
        <splash density="port-hdpi" src="res/android/splash/Splash Screen Android Large negro2.png" />
        <splash density="port-ldpi" src="res/android/splash/Splash Screen Android Small negro2.png" />
        <splash density="port-mdpi" src="res/android/splash/Splash Screen Android Medium negro2.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/ios/icon/Icon-60@3x.png" width="180" />
        <icon height="60" src="res/ios/icon/Icon-60.png" width="60" />
        <icon height="120" src="res/ios/icon/Icon-60@2x.png" width="120" />
        <icon height="76" src="res/ios/icon/Icon-76.png" width="76" />
        <icon height="152" src="res/ios/icon/Icon-76@2x.png" width="152" />
        <icon height="40" src="res/ios/icon/Icon-Small-40.png" width="40" />
        <icon height="80" src="res/ios/icon/Icon-Small-40@2x.png" width="80" />
        <icon height="72" src="res/ios/icon/Icon-72.png" width="72" />
        <icon height="144" src="res/ios/icon/Icon-72@2x.png" width="144" />
        <icon height="29" src="res/ios/icon/Icon-Small.png" width="29" />
        <icon height="58" src="res/ios/icon/Icon-Small@2x.png" width="58" />
        <icon height="50" src="res/ios/icon/Icon-Small-50.png" width="50" />
        <icon height="100" src="res/ios/icon/Icon-Small-50@2x.png" width="100" />
        <splash height="480" src="res/ios/splash/00-splash-iphone3.png" width="320" />
        <splash height="960" src="res/ios/splash/00-splash-iphone4@2x.png" width="640" />
        <splash height="1136" src="res/ios/splash/00-splash-iphone5-568h@3x.png" width="640" />
        <splash height="1334" src="res/ios/splash/00-splash-iphone6@2x.png" width="750" />
        <splash height="2208" src="res/ios/splash/00-splash-iphone6plus@3x.png" width="1242" />
        <hook src="hooks/add_ios_push_entitlements.sh" type="after_build" />
        <hook src="hooks/add_ios_push_entitlements.sh" type="after_platform_add" />
    </platform>
    <allow-navigation href="*://200.53.142.183:9998/*" />
    <allow-navigation href="*://humaneland.net/*" />
    <allow-navigation href="*://api.notificationscloud.com/*" />
    <engine name="android" spec="~6.2.3" />
    <plugin name="cordova-plugin-app-preferences" spec="~0.99.2" />
    <plugin name="cordova-plugin-device" spec="~1.1.1" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="https://github.com/Xzya/cordova-plugin-inappbrowser.git" />
    <plugin name="cordova-plugin-network-information" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.2" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-x-toast" spec="~2.5.0" />
    <plugin name="phonegap-plugin-push" spec="~1.9.2">
        <variable name="SENDER_ID" value="81216059519" />
    </plugin>
</widget>
